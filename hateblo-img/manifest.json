{
  "author": "Pocket Systems",
  "background": {
    "scripts": [ "js/storage-health.js", "js/ga/ga-tracker.js", "js/hatena/blog-image-core.js", "js/background.js" ],
    "persistent": false
  },
  "browser_action": {
    "default_icon": {
      "19": "images/hateblo_img19.png",
      "38": "images/hateblo_img38.png"
    },
    "default_title": "はてブロimageTitle",
    "default_popup": "html/hatena/blog-image-popup.html"
  },
  "content_scripts": [
    {
      "all_frames": true,
      "css": [ "css/hatena/blog-image.css" ],
      "js": [ "js/ga/ga-tracker.js", "js/hatena/blog-image.js" ],
      "matches": [ "http://blog.hatena.ne.jp/*/*/edit*" ],
      "run_at": "document_start"
    }
  ],
  "content_security_policy": "script-src 'self'; object-src 'self'; font-src 'self' https://fonts.googleapis.com https://fonts.gstatic.com; img-src 'self' data: https://ssl.google-analytics.com https://cdn-ak.f.st-hatena.com;",
  "description": "はてなブログにて画像を貼り付けるときにタイトルを編集できるようにするやつです。",
  "homepage_url": "http://psn.hatenablog.jp/entry/discover-hatena",
  "icons": {
    "16": "images/hateblo_img16.png",
    "48": "images/hateblo_img48.png",
    "128": "images/hateblo_img128.png"
  },
  "manifest_version": 2,
  "name": "はてブロimageTitle",
  "options_ui": {
    "chrome_style": true,
    "open_in_tab": true,
    "page": "html/hatena/blog-image-options.html"
  },
  "permissions": [
    "alarms",
    "storage",
    "activeTab",
    "tabs",
    "notifications",
    "http://blog.hatena.ne.jp/*"
  ],
  "short_name": "はてブロimageTitle",
  "version": "0.0.1.19",
  "version_name": "0.0.1.19 bata20161102"
}
