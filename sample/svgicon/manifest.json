{
  "background": {
    "scripts": [
      "js/background.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_icon": {
      "19": "img/icon_19.png"
    },
    "default_title": "svg test"
  },
  "content_security_policy": "script-src 'self'; object-src 'self'; img-src 'self' data:; media-src *;",
  "description": "svgのアイコンを表示させるテスト。",
  "homepage_url": "http://psn.hatenablog.jp/entry/discover-hatena",
  "icons": {
    "128": "img/img_16.png",
    "16": "img/img_16.png",
    "48": "img/img_16.png"
  },
  "manifest_version": 2,
  "name": "SVG icon test",
  "permissions": [
    "tabs",
    "alarms"
  ],
  "short_name": "svg icon",
  "version": "0.0.0.1"
}
